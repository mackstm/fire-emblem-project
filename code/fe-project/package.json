{
  "main": "electron/main.js",
  "name": "fe-project",
  "version": "0.0.0",
  "description": "Fire Emblem App built with Vue and Electron",
  "author": "Nabil Leon Alvarez & Jose Maximiliano Boada Martin",
  "private": true,
  "type": "commonjs",
  "build": {
    "appId": "com.feapp.electron",
    "productName": "feapp",
    "files": [
      "dist/**/*",
      "electron/main.js",
      "package.json"
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "appimage"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "electron:dev": "electron ./electron/main.ts",
    "electron:build": "tsc electron/main.ts && electron-builder"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.7",
    "axios": "^1.7.9",
    "vue": "^3.5.13",
    "vuetify": "^3.7.12"
  },
  "devDependencies": {
    "@electron/remote": "^2.1.2",
    "@eslint/js": "^9.20.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/eslint-plugin": "1.1.25",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "electron": "^34.2.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9.20.1",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.15.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.2",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.0.7",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.0.11",
    "vite-plugin-vue-devtools": "^7.7.0",
    "vitest": "^3.0.2",
    "vue-docgen-cli": "^4.79.0",
    "vue-tsc": "^2.2.0"
  }
}
